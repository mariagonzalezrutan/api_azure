{
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/main.py"
      }
    ],
    "redirects": [
        {
          "source": "/(.*)",
          "destination": "https://api-azure.vercel.app/$1",
          "permanent": true
        }
      ]
  }
  